{
  "name": "pc-boilerplate",
  "version": "0.1.0",
  "description": "pc-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack server --hot --open --env env=development",
    "build": "cross-env BABEL_ENV=production webpack --progress --env env=production",
    "prettier": "prettier src/**/*.js --write",
    "deploy": "npm run build && scp ./build/* ubuntu@domain:/home/ubuntu/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "yarn prettier",
      "git add"
    ]
  },
  "author": {
    "name": ""
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.8",
    "@babel/register": "^7.16.9",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "dayjs": "^1.10.7",
    "dx-mock": "^0.1.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.7",
    "mini-css-extract-plugin": "^2.4.6",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "jquery": "3.6.0",
    "lodash.throttle": "^4.1.1",
    "normalize.css": "^8.0.1"
  }
}
